<h1>Editing timesheet</h1>

<% if @timesheet.errors.any? %>
    <div id="error_explanation">
        <ul>
            <% @timesheet.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
        </ul>
    </div>
<% end %>

Employee: <%= @timesheet.employee.jira_username %><br>
End Date: <%= @timesheet.end_date %><br>

<%= render :partial => 'form' %>

<p>
<%= link_to 'Show', @timesheet %> |
<%= link_to 'Back', timesheets_path %>
